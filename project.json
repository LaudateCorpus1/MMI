            },
            "dependencies": {
                "System.Runtime.CompilerServices.VisualC": "4.0.0-rc2-24027",
                "System.Runtime.Serialization.Xml": "4.1.1-rc2-24027",
                "System.Threading.ThreadPool": "4.0.10-rc2-24027"
            }
        },
        "net451": {
        }
    }
